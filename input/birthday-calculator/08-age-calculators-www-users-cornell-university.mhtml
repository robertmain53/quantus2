        <h1><!-- #BeginEditable "Title" -->Age=20
                  Calculator<!-- #EndEditable --></h1>
        <!-- #BeginEditable "Body" -->=20
        <p>Calculate the age based on the Date of Birth and another date (d=
efault=20
          is the current date).</p>
        <form action=3D"http://www-users.med.cornell.edu/~spon/picu/calc/ag=
ecalc.htm" name=3D"Age">
          <table border=3D"0" cellpadding=3D"0" align=3D"center">
            <tbody><tr>=20
              <td>Date of Birth</td>
              <td></td>
              <td>=20
                <input type=3D"text" name=3D"DoB" size=3D"10">
              </td>
            </tr>
            <tr>=20
              <td>Age at This Date</td>
              <td></td>
              <td>=20
                <input type=3D"text" name=3D"DoC" size=3D"10">
              </td>
            </tr>
            <tr>=20
              <td colspan=3D"3">=20
                <div align=3D"center">=20
                  <input type=3D"button" name=3D"CalculateAge" value=3D"Cal=
culate">
                  <input type=3D"button" name=3D"ClearSome" value=3D"Clear"=
>
                </div>
              </td>
            </tr>
            <tr>=20
              <td>=20
                <div align=3D"right">Age&nbsp;</div>
              </td>
              <td> =3D&nbsp;</td>
              <td>=20
                <input type=3D"text" name=3D"AgeYears" size=3D"6">
                years=20
                <input type=3D"text" name=3D"AgeRmdr" size=3D"5">
                months</td>
            </tr>
            <tr>=20
              <td></td>
              <td>=3D&nbsp;</td>
              <td>=20
                <input type=3D"text" name=3D"AgeMonth" size=3D"6">
                months</td>
            </tr>
            <tr>=20
              <td></td>
              <td>=3D&nbsp;</td>
              <td>=20
                <input type=3D"text" name=3D"AgeWeeks" size=3D"6">
                weeks</td>
            </tr>
            <tr>=20
              <td></td>
              <td>=3D&nbsp;</td>
              <td>=20
                <input type=3D"text" name=3D"AgeDays" size=3D"6">
                days</td>
            </tr>
          </tbody></table>

        </form>
