#mara { flex-direction: column; }

#popular-road-distances, #popular-track-distances, #run-time-value { displa=
y: none; }

#run-distance-input { width: 6em; }

.small-header { text-align: center; }

#general-results, .list.prediction-table-small { width: auto; }

@media only screen and (min-width: 475px) {
  #run-distance-input { margin-top: 1em; }
}

@media only screen and (min-width: 1024px) {
  #distancess, .prediction-table-small, .small-header { display: none; }
  #run-time-value { display: block; }
  #run-time-value input { width: 8em; }
  #popular-road-distances, #popular-track-distances { display: table; }
}
