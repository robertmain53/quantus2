          <span>Currency</span>
          <input id=3D"sp_sym" value=3D"$" maxlength=3D"3" style=3D"padding=
:10px;border:1px solid #d1d5db;border-radius:10px;">
        </label>
        <label style=3D"display:flex;flex-direction:column;gap:6px;">
          <span>List Price (per unit)</span>
          <input type=3D"number" inputmode=3D"decimal" step=3D"any" id=3D"s=
p_list" placeholder=3D"e.g., 129.99" style=3D"padding:10px;border:1px solid=
 #d1d5db;border-radius:10px;">
        </label>
        <label style=3D"display:flex;flex-direction:column;gap:6px;">
          <span>Quantity</span>
          <input type=3D"number" inputmode=3D"decimal" step=3D"any" id=3D"s=
p_qty" value=3D"1" style=3D"padding:10px;border:1px solid #d1d5db;border-ra=
dius:10px;">
        </label>
      </div>

      <!-- Discount A -->
      <fieldset style=3D"border:1px solid #e5e7eb;border-radius:12px;paddin=
g:12px;">
        <legend style=3D"padding:0 6px;font-weight:600;">Discount A</legend=
>
        <div style=3D"display:grid;grid-template-columns:repeat(3,1fr);gap:=
12px;">
          <label style=3D"display:flex;flex-direction:column;gap:6px;">
            <span>Value</span>
            <input type=3D"number" inputmode=3D"decimal" step=3D"any" id=3D=
"sp_a_val" placeholder=3D"e.g., 20 or 15" style=3D"padding:10px;border:1px =
solid #d1d5db;border-radius:10px;">
          </label>
          <label style=3D"display:flex;flex-direction:column;gap:6px;">
            <span>Type</span>
            <select id=3D"sp_a_type" style=3D"padding:10px;border:1px solid=
 #d1d5db;border-radius:10px;">
              <option value=3D"percent">% off</option>
              <option value=3D"amount">$ off</option>
            </select>
          </label>
          <label style=3D"display:flex;align-items:center;gap:8px;">
            <input type=3D"checkbox" id=3D"sp_a_per_unit" checked=3D"">
            <span>Apply per unit (checked) or to subtotal</span>
          </label>
        </div>
      </fieldset>

      <!-- Discount B (stacked) -->
      <details style=3D"color:#374151;">
        <summary style=3D"cursor:pointer;font-weight:600;">Add Discount B (=
stacked)</summary>
