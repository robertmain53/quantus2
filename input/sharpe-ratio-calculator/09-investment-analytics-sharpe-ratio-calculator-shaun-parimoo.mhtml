@charset "utf-8";

body { overflow-x: hidden; font-family: "Roboto Slab", "Helvetica Neue", He=
lvetica, Arial, sans-serif; }

p { line-height: 1.75; }

a { color: rgb(105, 179, 162); }

a:hover { color: rgb(37, 22, 103); }

.text-primary { color: rgb(105, 179, 162) !important; }

h1, h2, h3, h4, h5, h6 { font-weight: 10; font-family: Montserrat, "Helveti=
ca Neue", Helvetica, Arial, sans-serif; }

section { padding: 100px 0px; }

section h2.section-heading { font-size: 40px; margin-top: 0px; margin-botto=
m: 15px; }

section h3.section-subheading { font-size: 16px; font-weight: 400; font-sty=
le: italic; margin-bottom: 45px; text-transform: none; font-family: "Droid =
Serif", "Helvetica Neue", Helvetica, Arial, sans-serif; }

@media (min-width: 768px) {
  section { padding: 150px 0px; }
  .section-anti-padding { padding-top: 0px; padding-bottom: 0px; }
}

