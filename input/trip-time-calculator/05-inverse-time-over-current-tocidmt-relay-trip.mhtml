"checkbox"], select { padding: 6px; display: inline-block; border: 1px soli=
d rgb(204, 204, 204); box-sizing: border-box; margin-top: 5px; margin-botto=
m: 5px; font-size: 20px; width: 100%; border-radius: 4px; background-color:=
 rgb(255, 255, 255); }

