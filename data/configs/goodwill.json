{
  "version": "1.0.0",
  "metadata": {
    "title": "Goodwill Donation & Valuation Calculator",
    "description": "Estimate the fair-market value of donated household items or calculate a business goodwill estimate. Choose an itemized valuation, use your donation receipt, or run a simple business goodwill estimate. Includes accuracy caveats and regulatory references."
  },
  "logic": {
    "type": "advanced",
    "defaultMethod": "itemized",
    "methods": {
      "itemized": {
        "label": "Itemized donation (common categories)",
        "description": "Estimate value by entering quantities and per-item fair-market values for common donation categories. Use the 'Other items (total)' field for items not listed.",
        "variables": {
          "clothing_total": {
            "expression": "clothing_qty * clothing_value_each",
            "dependencies": [
              "clothing_qty",
              "clothing_value_each"
            ],
            "label": "Clothing subtotal",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "electronics_total": {
            "expression": "electronics_qty * electronics_value_each",
            "dependencies": [
              "electronics_qty",
              "electronics_value_each"
            ],
            "label": "Electronics subtotal",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "furniture_total": {
            "expression": "furniture_qty * furniture_value_each",
            "dependencies": [
              "furniture_qty",
              "furniture_value_each"
            ],
            "label": "Furniture subtotal",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "books_total": {
            "expression": "books_qty * books_value_each",
            "dependencies": [
              "books_qty",
              "books_value_each"
            ],
            "label": "Books subtotal",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "shoes_total": {
            "expression": "shoes_qty * shoes_value_each",
            "dependencies": [
              "shoes_qty",
              "shoes_value_each"
            ],
            "label": "Shoes subtotal",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "other_total": {
            "expression": "other_items_total",
            "dependencies": [
              "other_items_total"
            ],
            "label": "Other items (total)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "impact_factor": {
            "expression": "0.60",
            "dependencies": [],
            "label": "Estimated program impact factor",
            "format": "decimal",
            "display": false
          },
          "total_value": {
            "expression": "clothing_total + electronics_total + furniture_total + books_total + shoes_total + other_total",
            "dependencies": [
              "clothing_total",
              "electronics_total",
              "furniture_total",
              "books_total",
              "shoes_total",
              "other_total"
            ],
            "label": "Estimated total fair-market value",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "estimated_tax_deduction": {
            "expression": "total_value",
            "dependencies": [
              "total_value"
            ],
            "label": "Suggested tax deduction (informational)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "estimated_impact": {
            "expression": "total_value * impact_factor",
            "dependencies": [
              "total_value",
              "impact_factor"
            ],
            "label": "Estimated program impact (internal estimate)",
            "unit": "USD",
            "format": "currency",
            "display": true
          }
        },
        "outputs": [
          {
            "id": "total_value",
            "label": "Estimated total fair‑market value (USD)",
            "variable": "total_value",
            "unit": "USD",
            "format": "currency"
          },
          {
            "id": "estimated_tax_deduction",
            "label": "Suggested tax deduction (informational)",
            "variable": "estimated_tax_deduction",
            "unit": "USD",
            "format": "currency"
          },
          {
            "id": "estimated_impact",
            "label": "Estimated program impact (USD)",
            "variable": "estimated_impact",
            "unit": "USD",
            "format": "currency"
          }
        ]
      },
      "receipt_total": {
        "label": "Use donation receipt total",
        "description": "Enter the total donation value shown on your donation receipt to use as the estimated fair-market value.",
        "variables": {
          "receipt_total_amount": {
            "expression": "receipt_total_amount",
            "dependencies": [
              "receipt_total_amount"
            ],
            "label": "Receipt total",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "impact_factor": {
            "expression": "0.60",
            "dependencies": [],
            "label": "Estimated program impact factor",
            "format": "decimal",
            "display": false
          },
          "total_value": {
            "expression": "receipt_total_amount",
            "dependencies": [
              "receipt_total_amount"
            ],
            "label": "Estimated total fair-market value",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "estimated_tax_deduction": {
            "expression": "total_value",
            "dependencies": [
              "total_value"
            ],
            "label": "Suggested tax deduction (informational)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "estimated_impact": {
            "expression": "total_value * impact_factor",
            "dependencies": [
              "total_value",
              "impact_factor"
            ],
            "label": "Estimated program impact (internal estimate)",
            "unit": "USD",
            "format": "currency",
            "display": true
          }
        },
        "outputs": [
          {
            "id": "total_value",
            "label": "Estimated total fair‑market value (USD)",
            "variable": "total_value",
            "unit": "USD",
            "format": "currency"
          },
          {
            "id": "estimated_tax_deduction",
            "label": "Suggested tax deduction (informational)",
            "variable": "estimated_tax_deduction",
            "unit": "USD",
            "format": "currency"
          },
          {
            "id": "estimated_impact",
            "label": "Estimated program impact (USD)",
            "variable": "estimated_impact",
            "unit": "USD",
            "format": "currency"
          }
        ]
      },
      "business_goodwill": {
        "label": "Simple business goodwill estimate",
        "description": "A basic business goodwill estimate using Seller's Discretionary Earnings (SDE) × multiplier, less tangible net assets. This is an overview calculation and not a formal appraisal.",
        "variables": {
          "sde": {
            "expression": "sde",
            "dependencies": [
              "sde"
            ],
            "label": "Seller's Discretionary Earnings (SDE)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "multiplier": {
            "expression": "multiplier",
            "dependencies": [
              "multiplier"
            ],
            "label": "Market multiplier (typical ranges 1.0–4.0)",
            "format": "decimal",
            "display": true
          },
          "tangible_assets": {
            "expression": "tangible_assets",
            "dependencies": [
              "tangible_assets"
            ],
            "label": "Net tangible assets",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "goodwill_value": {
            "expression": "sde * multiplier - tangible_assets",
            "dependencies": [
              "sde",
              "multiplier",
              "tangible_assets"
            ],
            "label": "Estimated goodwill value",
            "unit": "USD",
            "format": "currency",
            "display": true
          }
        },
        "outputs": [
          {
            "id": "goodwill_value",
            "label": "Estimated goodwill (USD)",
            "variable": "goodwill_value",
            "unit": "USD",
            "format": "currency"
          }
        ]
      }
    }
  },
  "form": {
    "fields": [
      {
        "id": "calculation_method",
        "label": "Calculation method",
        "type": "select",
        "default": "itemized",
        "options": [
          {
            "label": "Itemized donation (common categories)",
            "value": "itemized"
          },
          {
            "label": "Donation receipt total",
            "value": "receipt_total"
          },
          {
            "label": "Business goodwill estimate",
            "value": "business_goodwill"
          }
        ]
      }
    ],
    "sections": [
      {
        "id": "section_itemized",
        "label": "Itemized donation inputs",
        "show_when": {
          "field": "calculation_method",
          "is": "itemized"
        },
        "fields": [
          {
            "id": "clothing_qty",
            "label": "Clothing — quantity",
            "type": "integer",
            "default": 2,
            "min": 0,
            "step": 1
          },
          {
            "id": "clothing_value_each",
            "label": "Clothing — value per item (USD)",
            "type": "number",
            "default": 6,
            "min": 0,
            "step": 0.01
          },
          {
            "id": "electronics_qty",
            "label": "Small electronics — quantity",
            "type": "integer",
            "default": 0,
            "min": 0,
            "step": 1
          },
          {
            "id": "electronics_value_each",
            "label": "Small electronics — value per item (USD)",
            "type": "number",
            "default": 15,
            "min": 0,
            "step": 0.01
          },
          {
            "id": "furniture_qty",
            "label": "Furniture — quantity",
            "type": "integer",
            "default": 0,
            "min": 0,
            "step": 1
          },
          {
            "id": "furniture_value_each",
            "label": "Furniture — value per item (USD)",
            "type": "number",
            "default": 45,
            "min": 0,
            "step": 0.01
          },
          {
            "id": "books_qty",
            "label": "Books — quantity",
            "type": "integer",
            "default": 3,
            "min": 0,
            "step": 1
          },
          {
            "id": "books_value_each",
            "label": "Books — value per item (USD)",
            "type": "number",
            "default": 2,
            "min": 0,
            "step": 0.01
          },
          {
            "id": "shoes_qty",
            "label": "Shoes — quantity",
            "type": "integer",
            "default": 1,
            "min": 0,
            "step": 1
          },
          {
            "id": "shoes_value_each",
            "label": "Shoes — value per item (USD)",
            "type": "number",
            "default": 8,
            "min": 0,
            "step": 0.01
          },
          {
            "id": "other_items_total",
            "label": "Other items — total estimated value (USD)",
            "type": "number",
            "default": 0,
            "min": 0,
            "step": 0.01
          }
        ]
      },
      {
        "id": "section_receipt",
        "label": "Receipt-based input",
        "show_when": {
          "field": "calculation_method",
          "is": "receipt_total"
        },
        "fields": [
          {
            "id": "receipt_total_amount",
            "label": "Total value on donation receipt (USD)",
            "type": "number",
            "default": 25,
            "min": 0,
            "step": 0.01
          }
        ]
      },
      {
        "id": "section_business",
        "label": "Business goodwill inputs",
        "show_when": {
          "field": "calculation_method",
          "is": "business_goodwill"
        },
        "fields": [
          {
            "id": "sde",
            "label": "Seller's Discretionary Earnings (SDE) — annual (USD)",
            "type": "number",
            "default": 50000,
            "min": 0,
            "step": 1
          },
          {
            "id": "multiplier",
            "label": "Market multiplier (typical 1.0–4.0)",
            "type": "number",
            "default": 2.0,
            "min": 0.1,
            "step": 0.01
          },
          {
            "id": "tangible_assets",
            "label": "Net tangible assets (USD)",
            "type": "number",
            "default": 10000,
            "min": 0,
            "step": 1
          }
        ]
      }
    ]
  },
  "page_content": {
    "introduction": [
      "This calculator helps estimate the fair-market value of donated household items or provides a simple business goodwill estimate. Use the itemized method to enter common categories (clothing, electronics, furniture, books, shoes, or an 'other' total). Alternatively, use your donation receipt total or the basic business goodwill method.",
      "Results are informational estimates to help with recordkeeping and tax planning. They are not formal appraisals. For tax reporting and large or unusual donations consult official tax guidance or a qualified appraiser."
    ],
    "methodology": [
      "Itemized method multiplies quantity × per-item fair-market value for each category, then sums subtotals to produce a total estimated fair-market value. A fixed internal 'impact factor' approximates program benefit from donated goods; this is an institution-specific estimate and is shown separately.",
      "Receipt-based method uses the total shown on the organization's receipt as the estimated fair-market value. The business goodwill method applies a multiplier to Seller's Discretionary Earnings (SDE) and subtracts net tangible assets to provide a high-level goodwill estimate.",
      "Calculations follow standard numeric safety and traceability practices. Data handling and integrity recommendations are aligned with NIST guidance for trustworthy data and recordkeeping."
    ],
    "how_is_calculated": [
      "Itemized total = sum over categories of (quantity × value per item) + other items total.",
      "Receipt total method uses: Estimated value = Receipt total amount.",
      "Business goodwill (simple) = SDE × multiplier − net tangible assets.",
      "Estimated program impact (informational) = Estimated total × impact factor (default 0.60). This factor is a non-regulatory estimate intended to show program-side value derived from donations and can be adjusted by organizations."
    ],
    "examples": [
      "Example 1 — Itemized: 3 shirts at $5 each + 2 books at $2 each + other $10 total → Total = (3×5) + (2×2) + 10 = $29.",
      "Example 2 — Receipt: If your donation receipt shows $75, enter $75 to use that as the estimate.",
      "Example 3 — Business goodwill: SDE $80,000 × multiplier 2.0 − tangible assets $20,000 = $140,000 goodwill estimate."
    ],
    "faqs": [
      {
        "question": "Is this an official tax valuation?",
        "answer": "No. This tool provides informational estimates for recordkeeping and planning. For tax deductions consult IRS rules and, for items or donations above IRS thresholds, seek a qualified appraisal."
      },
      {
        "question": "When do I need a formal appraisal?",
        "answer": "Per IRS guidance, noncash contributions with a claimed value over $500 may require additional forms and documentation. Items or collections with values exceeding $5,000 generally require a qualified appraisal (see IRS Publication 561 and Form 8283 instructions)."
      },
      {
        "question": "How accurate are the per-item default values?",
        "answer": "Default per-item values are conservative examples only. Fair-market value depends on condition, age, and market demand. Review item condition and adjust per-item values before finalizing records."
      },
      {
        "question": "Can I rely on the 'estimated program impact' for official reporting?",
        "answer": "The program impact figure is an internal, illustrative estimate and is not an accounting or tax metric. Use only for internal planning or donor communications with appropriate disclaimers."
      },
      {
        "question": "Is electronic data handled securely?",
        "answer": "This tool is informational; deployers should follow NIST SP 800-series recommendations for data integrity and secure handling. Avoid entering sensitive personal or financial data beyond what is necessary for the estimate."
      }
    ],
