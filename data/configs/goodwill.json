    ],
    "glossary": [
      {
        "term": "Fair-market value",
        "definition": "The price at which property would change hands between a willing buyer and a willing seller, neither under compulsion to buy or sell and both having reasonable knowledge of relevant facts."
      },
      {
        "term": "Seller's Discretionary Earnings (SDE)",
        "definition": "A measure of business cash flow often used in small-business valuations, typically accounting for owner compensation and discretionary expenses."
      },
      {
        "term": "Form 8283",
        "definition": "IRS form used to report noncash charitable contributions when the deduction for an item or group of similar items exceeds certain thresholds."
      },
      {
        "term": "Impact factor",
        "definition": "An internal multiplier used to translate donated item value into an estimated program-side benefit. Not a regulated or audited metric."
      }
    ]
  },
