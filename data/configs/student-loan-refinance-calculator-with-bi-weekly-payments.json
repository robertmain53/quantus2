          "total_interest_bi_c": {
            "expression": "biweekly_payment_c * n_periods_bi_c - principal - refinance_fee",
            "dependencies": [
              "biweekly_payment_c",
              "n_periods_bi_c",
              "principal",
              "refinance_fee"
            ],
            "label": "Total interest (bi‑weekly amortized, comparison)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "half_monthly_payment_c": {
            "expression": "(if(n_months_c>0 && monthly_rate_c>0, principal * (monthly_rate_c * pow(1+monthly_rate_c, n_months_c)) / (pow(1+monthly_rate_c, n_months_c) - 1), if(n_months_c>0, principal / n_months_c, 0)) + refinance_fee / n_months_c) / 2 + extra_payment",
            "dependencies": [
