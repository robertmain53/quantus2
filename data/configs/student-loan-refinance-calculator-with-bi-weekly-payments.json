{
  "version": "1.0.0",
  "metadata": {
    "title": "Student Loan Refinance Calculator — Monthly vs Bi‑Weekly (including accelerated bi‑weekly)",
    "description": "Compare a standard refinanced monthly amortization to two bi‑weekly options (true bi‑weekly amortization and accelerated bi‑weekly via half‑monthly payments). Estimates include periodic payment, total interest, payoff time, and savings. Includes accuracy caveats and references to engineering and measurement standards."
  },
  "logic": {
    "type": "advanced",
    "defaultMethod": "comparison",
    "methods": {
      "monthly": {
        "label": "Standard Monthly Refinance",
        "description": "Calculates a standard monthly amortizing payment for the refinanced principal and term.",
        "variables": {
          "annual_rate_decimal": {
            "expression": "annual_interest_rate / 100",
            "dependencies": [
              "annual_interest_rate"
            ],
            "label": "Annual interest rate (decimal)",
            "format": "decimal",
            "display": false
          },
          "monthly_rate": {
            "expression": "annual_rate_decimal / 12",
            "dependencies": [
              "annual_rate_decimal"
            ],
            "label": "Monthly interest rate",
            "format": "decimal",
            "display": false
          },
          "n_months": {
            "expression": "term_years * 12",
            "dependencies": [
              "term_years"
            ],
            "label": "Number of monthly payments",
            "unit": "months",
            "format": "integer",
            "display": false
          },
          "monthly_payment": {
            "expression": "if(n_months>0 && monthly_rate>0, principal * (monthly_rate * pow(1+monthly_rate, n_months)) / (pow(1+monthly_rate, n_months) - 1), if(n_months>0, principal / n_months, 0)) + refinance_fee / n_months + extra_payment",
            "dependencies": [
              "principal",
              "monthly_rate",
              "n_months",
              "refinance_fee",
              "extra_payment"
            ],
            "label": "Estimated monthly payment (including allocated refinance fee and extra payment)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "total_paid_monthly": {
            "expression": "monthly_payment * n_months",
            "dependencies": [
              "monthly_payment",
              "n_months"
            ],
            "label": "Total paid over life (monthly)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "total_interest_monthly": {
            "expression": "total_paid_monthly - principal - refinance_fee",
            "dependencies": [
              "total_paid_monthly",
              "principal",
              "refinance_fee"
            ],
            "label": "Total interest paid (monthly)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "payoff_years_monthly": {
            "expression": "n_months / 12",
            "dependencies": [
              "n_months"
            ],
            "label": "Payoff time (years) if paid monthly",
            "unit": "years",
            "format": "decimal",
            "display": true
          }
        },
        "outputs": [
          {
            "id": "monthly_payment",
            "label": "Monthly payment",
            "variable": "monthly_payment",
            "unit": "USD",
            "format": "currency"
          },
          {
            "id": "total_interest_monthly",
            "label": "Total interest (monthly)",
            "variable": "total_interest_monthly",
            "unit": "USD",
            "format": "currency"
          },
          {
            "id": "payoff_years_monthly",
            "label": "Payoff time (years, monthly)",
            "variable": "payoff_years_monthly",
            "unit": "years",
            "format": "decimal"
          }
        ]
      },
      "biweekly_amortized": {
        "label": "True Bi‑Weekly Amortization (26 periods/year)",
        "description": "Amortizes using 26 equal periods per year. Useful when a lender compounds interest to bi‑weekly periods.",
        "variables": {
          "annual_rate_decimal_bi": {
            "expression": "annual_interest_rate / 100",
            "dependencies": [
              "annual_interest_rate"
            ],
            "label": "Annual interest rate (decimal)",
            "format": "decimal",
            "display": false
          },
          "period_rate_bi": {
            "expression": "annual_rate_decimal_bi / 26",
            "dependencies": [
              "annual_rate_decimal_bi"
            ],
            "label": "Bi‑weekly period rate",
            "format": "decimal",
            "display": false
          },
          "n_periods_bi": {
            "expression": "term_years * 26",
            "dependencies": [
              "term_years"
            ],
            "label": "Number of bi‑weekly periods",
            "unit": "periods",
            "format": "integer",
            "display": false
          },
          "biweekly_payment": {
            "expression": "if(n_periods_bi>0 && period_rate_bi>0, principal * (period_rate_bi * pow(1+period_rate_bi, n_periods_bi)) / (pow(1+period_rate_bi, n_periods_bi) - 1), if(n_periods_bi>0, principal / n_periods_bi, 0)) + refinance_fee / n_periods_bi + extra_payment",
            "dependencies": [
              "principal",
              "period_rate_bi",
              "n_periods_bi",
              "refinance_fee",
              "extra_payment"
            ],
            "label": "Bi‑weekly payment (26 periods/year)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "total_paid_bi": {
            "expression": "biweekly_payment * n_periods_bi",
            "dependencies": [
              "biweekly_payment",
              "n_periods_bi"
            ],
            "label": "Total paid over life (bi‑weekly amortized)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "total_interest_bi": {
            "expression": "total_paid_bi - principal - refinance_fee",
            "dependencies": [
              "total_paid_bi",
              "principal",
              "refinance_fee"
            ],
            "label": "Total interest paid (bi‑weekly amortized)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "payoff_years_bi": {
            "expression": "n_periods_bi / 26",
            "dependencies": [
              "n_periods_bi"
            ],
            "label": "Payoff time (years, bi‑weekly amortized)",
            "unit": "years",
            "format": "decimal",
            "display": true
          }
        },
        "outputs": [
          {
            "id": "biweekly_payment",
            "label": "Bi‑weekly payment (26/year)",
            "variable": "biweekly_payment",
            "unit": "USD",
            "format": "currency"
          },
          {
            "id": "total_interest_bi",
            "label": "Total interest (bi‑weekly amortized)",
            "variable": "total_interest_bi",
            "unit": "USD",
            "format": "currency"
          },
          {
            "id": "payoff_years_bi",
            "label": "Payoff time (years, bi‑weekly amortized)",
            "variable": "payoff_years_bi",
            "unit": "years",
            "format": "decimal"
          }
        ]
      },
      "accelerated_biweekly": {
        "label": "Accelerated Bi‑Weekly (half of monthly every 2 weeks)",
        "description": "Models making half of the standard monthly payment every two weeks (26 payments per year), which results in one extra monthly payment per year and typically shortens the term.",
        "variables": {
          "annual_rate_decimal_acc": {
            "expression": "annual_interest_rate / 100",
            "dependencies": [
              "annual_interest_rate"
            ],
            "label": "Annual interest rate (decimal)",
            "format": "decimal",
            "display": false
          },
          "monthly_rate_acc": {
            "expression": "annual_rate_decimal_acc / 12",
            "dependencies": [
              "annual_rate_decimal_acc"
            ],
            "label": "Monthly rate (for baseline monthly payment)",
            "format": "decimal",
            "display": false
          },
          "n_months_acc": {
            "expression": "term_years * 12",
            "dependencies": [
              "term_years"
            ],
            "label": "Number of months (baseline)",
            "unit": "months",
            "format": "integer",
            "display": false
          },
          "baseline_monthly_payment": {
            "expression": "if(n_months_acc>0 && monthly_rate_acc>0, principal * (monthly_rate_acc * pow(1+monthly_rate_acc, n_months_acc)) / (pow(1+monthly_rate_acc, n_months_acc) - 1), if(n_months_acc>0, principal / n_months_acc, 0)) + refinance_fee / n_months_acc",
            "dependencies": [
              "principal",
              "monthly_rate_acc",
              "n_months_acc",
              "refinance_fee"
            ],
            "label": "Baseline monthly payment (used to compute half‑monthly amount)",
            "unit": "USD",
            "format": "currency",
            "display": false
          },
          "half_monthly_payment": {
            "expression": "baseline_monthly_payment / 2 + extra_payment",
            "dependencies": [
              "baseline_monthly_payment",
              "extra_payment"
            ],
            "label": "Payment made every two weeks (half of baseline monthly, plus extra payment)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "period_rate_acc": {
            "expression": "annual_rate_decimal_acc / 26",
            "dependencies": [
              "annual_rate_decimal_acc"
            ],
            "label": "Bi‑weekly period rate (for accelerated calculation)",
            "format": "decimal",
            "display": false
          },
          "n_periods_to_payoff_acc": {
            "expression": "if(half_monthly_payment > principal * period_rate_acc, ln(half_monthly_payment / (half_monthly_payment - principal * period_rate_acc)) / ln(1 + period_rate_acc), 0)",
            "dependencies": [
              "half_monthly_payment",
              "principal",
              "period_rate_acc"
            ],
            "label": "Estimated number of bi‑weekly periods to payoff (accelerated)",
            "unit": "periods",
            "format": "decimal",
            "display": true
          },
          "total_paid_acc": {
            "expression": "half_monthly_payment * n_periods_to_payoff_acc",
            "dependencies": [
              "half_monthly_payment",
              "n_periods_to_payoff_acc"
            ],
            "label": "Total paid (accelerated bi‑weekly)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "total_interest_acc": {
            "expression": "total_paid_acc - principal - refinance_fee",
            "dependencies": [
              "total_paid_acc",
              "principal",
              "refinance_fee"
            ],
            "label": "Total interest paid (accelerated bi‑weekly)",
            "unit": "USD",
            "format": "currency",
            "display": true
          },
          "payoff_years_acc": {
            "expression": "n_periods_to_payoff_acc / 26",
            "dependencies": [
              "n_periods_to_payoff_acc"
            ],
            "label": "Payoff time (years, accelerated bi‑weekly)",
            "unit": "years",
            "format": "decimal",
            "display": true
          }
        },
        "outputs": [
          {
            "id": "half_monthly_payment",
            "label": "Bi‑weekly payment (accelerated; half of monthly)",
            "variable": "half_monthly_payment",
            "unit": "USD",
            "format": "currency"
          },
          {
            "id": "total_interest_acc",
            "label": "Total interest (accelerated bi‑weekly)",
            "variable": "total_interest_acc",
            "unit": "USD",
            "format": "currency"
          },
          {
            "id": "payoff_years_acc",
            "label": "Payoff time (years, accelerated)",
            "variable": "payoff_years_acc",
            "unit": "years",
            "format": "decimal"
          }
        ]
      },
      "comparison": {
        "label": "Comparison — Monthly vs Bi‑Weekly Options",
