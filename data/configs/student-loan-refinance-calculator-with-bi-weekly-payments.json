          "biweekly_payment_c": {
            "expression": "if(n_periods_bi_c>0 && period_rate_bi_c>0, principal * (period_rate_bi_c * pow(1+period_rate_bi_c, n_periods_bi_c)) / (pow(1+period_rate_bi_c, n_periods_bi_c) - 1), if(n_periods_bi_c>0, principal / n_periods_bi_c, 0)) + refinance_fee / n_periods_bi_c + extra_payment",
            "dependencies": [
