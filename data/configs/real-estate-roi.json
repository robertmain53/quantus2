{
  "version": "1.0.0",
  "metadata": {
    "title": "Real Estate ROI & Investment Analysis",
    "description": "Multi-method real estate investment calculator that estimates Cap Rate, Cash-on-Cash return, Total Return and an approximate IRR based on purchase, financing, income, expense and sale assumptions."
  },
  "logic": {
    "type": "advanced",
    "defaultMethod": "cash_on_cash",
    "methods": {
      "cash_on_cash": {
        "label": "Cash-on-Cash Return",
        "description": "Annual pre-tax cash return relative to cash invested (down payment + closing costs).",
        "variables": {
          "loan_amount": {
            "expression": "purchase_price - down_payment",
            "dependencies": [
              "purchase_price",
              "down_payment"
            ],
            "label": "Loan amount",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "monthly_rate": {
            "expression": "loan_interest_rate / 12 / 100",
            "dependencies": [
              "loan_interest_rate"
            ],
            "label": "Monthly interest rate",
            "format": "decimal",
            "display": false
          },
          "n_payments": {
            "expression": "loan_term_years * 12",
            "dependencies": [
              "loan_term_years"
            ],
            "label": "Number of monthly payments",
            "format": "integer",
            "display": false
          },
          "monthly_payment": {
            "expression": "loan_amount * (monthly_rate) / (1 - (1 + monthly_rate) ^ (-n_payments))",
            "dependencies": [
              "loan_amount",
              "monthly_rate",
              "n_payments"
            ],
            "label": "Estimated monthly mortgage payment",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "annual_gross_income": {
            "expression": "(monthly_rent * (1 - vacancy_rate / 100) + other_monthly_income) * 12",
            "dependencies": [
              "monthly_rent",
              "vacancy_rate",
              "other_monthly_income"
            ],
            "label": "Estimated annual gross income",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "operating_expenses_total": {
            "expression": "annual_operating_expenses + property_tax_annual + insurance_annual + hoa_annual",
            "dependencies": [
              "annual_operating_expenses",
              "property_tax_annual",
              "insurance_annual",
              "hoa_annual"
            ],
            "label": "Total annual operating expenses",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "net_operating_income": {
            "expression": "annual_gross_income - operating_expenses_total",
            "dependencies": [
              "annual_gross_income",
              "operating_expenses_total"
            ],
            "label": "Net operating income (NOI)",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "debt_service": {
            "expression": "monthly_payment * 12",
            "dependencies": [
              "monthly_payment"
            ],
            "label": "Annual debt service",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "annual_cash_flow": {
            "expression": "net_operating_income - debt_service - capital_expenditures_annual",
            "dependencies": [
              "net_operating_income",
              "debt_service",
              "capital_expenditures_annual"
            ],
            "label": "Estimated annual pre-tax cash flow",
            "unit": "currency",
            "format": "currency",
            "display": true
          },
          "closing_costs": {
            "expression": "purchase_price * closing_costs_percent / 100",
            "dependencies": [
              "purchase_price",
              "closing_costs_percent"
            ],
            "label": "Estimated closing costs",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "total_cash_invested": {
            "expression": "down_payment + closing_costs",
            "dependencies": [
              "down_payment",
              "closing_costs"
            ],
            "label": "Total initial cash invested",
            "unit": "currency",
            "format": "currency",
            "display": true
          },
          "cash_on_cash_return": {
            "expression": "annual_cash_flow / total_cash_invested",
            "dependencies": [
              "annual_cash_flow",
              "total_cash_invested"
            ],
            "label": "Cash-on-Cash Return",
            "unit": "%",
            "format": "percent",
            "display": true
          }
        },
        "outputs": [
          {
            "id": "cash_on_cash_return",
            "label": "Cash-on-Cash Return",
            "variable": "cash_on_cash_return",
            "unit": "%",
            "format": "percent"
          },
          {
            "id": "annual_cash_flow",
            "label": "Annual Cash Flow",
            "variable": "annual_cash_flow",
            "unit": "currency",
            "format": "currency"
          },
          {
            "id": "total_cash_invested",
            "label": "Total Cash Invested",
            "variable": "total_cash_invested",
            "unit": "currency",
            "format": "currency"
          }
        ]
      },
      "cap_rate": {
        "label": "Capitalization Rate (Cap Rate)",
        "description": "Net operating income divided by purchase price; excludes financing.",
        "variables": {
          "annual_gross_income": {
            "expression": "(monthly_rent * (1 - vacancy_rate / 100) + other_monthly_income) * 12",
            "dependencies": [
              "monthly_rent",
              "vacancy_rate",
              "other_monthly_income"
            ],
            "label": "Estimated annual gross income",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "operating_expenses_total": {
            "expression": "annual_operating_expenses + property_tax_annual + insurance_annual + hoa_annual",
            "dependencies": [
              "annual_operating_expenses",
              "property_tax_annual",
              "insurance_annual",
              "hoa_annual"
            ],
            "label": "Total annual operating expenses",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "net_operating_income": {
            "expression": "annual_gross_income - operating_expenses_total",
            "dependencies": [
              "annual_gross_income",
              "operating_expenses_total"
            ],
            "label": "Net operating income (NOI)",
            "unit": "currency",
            "format": "currency",
            "display": true
          },
          "cap_rate": {
            "expression": "net_operating_income / purchase_price",
            "dependencies": [
              "net_operating_income",
              "purchase_price"
            ],
            "label": "Capitalization rate",
            "unit": "%",
            "format": "percent",
            "display": true
          }
        },
        "outputs": [
          {
            "id": "cap_rate",
            "label": "Cap Rate",
            "variable": "cap_rate",
            "unit": "%",
            "format": "percent"
          },
          {
            "id": "net_operating_income",
            "label": "Net Operating Income (NOI)",
            "variable": "net_operating_income",
            "unit": "currency",
            "format": "currency"
          }
        ]
      },
      "total_return": {
        "label": "Total Return (Hold + Sale)",
        "description": "Estimate of cumulative cash flows plus net sale proceeds over the holding period, expressed as a percent of initial cash invested.",
        "variables": {
          "loan_amount": {
            "expression": "purchase_price - down_payment",
            "dependencies": [
              "purchase_price",
              "down_payment"
            ],
            "label": "Loan amount",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "monthly_rate": {
            "expression": "loan_interest_rate / 12 / 100",
            "dependencies": [
              "loan_interest_rate"
            ],
            "label": "Monthly interest rate",
            "format": "decimal",
            "display": false
          },
          "n_payments": {
            "expression": "loan_term_years * 12",
            "dependencies": [
              "loan_term_years"
            ],
            "label": "Number of monthly payments",
            "format": "integer",
            "display": false
          },
          "monthly_payment": {
            "expression": "loan_amount * (monthly_rate) / (1 - (1 + monthly_rate) ^ (-n_payments))",
            "dependencies": [
              "loan_amount",
              "monthly_rate",
              "n_payments"
            ],
            "label": "Estimated monthly mortgage payment",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "annual_cash_flow": {
            "expression": "((monthly_rent * (1 - vacancy_rate / 100) + other_monthly_income) * 12 - (annual_operating_expenses + property_tax_annual + insurance_annual + hoa_annual) - (monthly_payment * 12) - capital_expenditures_annual)",
            "dependencies": [
              "monthly_rent",
              "vacancy_rate",
              "other_monthly_income",
              "annual_operating_expenses",
              "property_tax_annual",
              "insurance_annual",
              "hoa_annual",
              "monthly_payment",
              "capital_expenditures_annual"
            ],
            "label": "Estimated annual pre-tax cash flow",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "cumulative_cash_flow": {
            "expression": "annual_cash_flow * hold_years",
            "dependencies": [
              "annual_cash_flow",
              "hold_years"
            ],
            "label": "Cumulative pre-tax cash flow over hold period",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "future_sale_price": {
            "expression": "purchase_price * (1 + annual_appreciation_percent / 100) ^ hold_years",
            "dependencies": [
              "purchase_price",
              "annual_appreciation_percent",
              "hold_years"
            ],
            "label": "Estimated sale price at time of sale",
            "unit": "currency",
            "format": "currency",
            "display": true
          },
          "selling_costs": {
            "expression": "future_sale_price * selling_costs_percent / 100",
            "dependencies": [
              "future_sale_price",
              "selling_costs_percent"
            ],
            "label": "Estimated selling costs (commissions, fees)",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "n_paid": {
            "expression": "hold_years * 12",
            "dependencies": [
              "hold_years"
            ],
            "label": "Number of payments made by sale",
            "format": "integer",
            "display": false
          },
          "remaining_loan_balance": {
            "expression": "loan_amount * (1 + monthly_rate) ^ n_paid - monthly_payment * ((1 + monthly_rate) ^ n_paid - 1) / monthly_rate",
            "dependencies": [
              "loan_amount",
              "monthly_rate",
              "n_paid",
              "monthly_payment"
            ],
            "label": "Estimated remaining loan balance at sale",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "net_sale_proceeds": {
            "expression": "future_sale_price - selling_costs - remaining_loan_balance",
            "dependencies": [
              "future_sale_price",
              "selling_costs",
              "remaining_loan_balance"
            ],
            "label": "Net sale proceeds after repaying loan and selling costs",
            "unit": "currency",
            "format": "currency",
            "display": true
          },
          "closing_costs": {
            "expression": "purchase_price * closing_costs_percent / 100",
            "dependencies": [
              "purchase_price",
              "closing_costs_percent"
            ],
            "label": "Estimated closing costs",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "total_cash_invested": {
            "expression": "down_payment + closing_costs",
            "dependencies": [
              "down_payment",
              "closing_costs"
            ],
            "label": "Total initial cash invested",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "total_profit": {
            "expression": "cumulative_cash_flow + net_sale_proceeds",
            "dependencies": [
              "cumulative_cash_flow",
              "net_sale_proceeds"
            ],
            "label": "Total pre-tax profit (cash flows + net sale proceeds)",
            "unit": "currency",
            "format": "currency",
            "display": true
          },
          "total_return_percent": {
            "expression": "total_profit / total_cash_invested",
            "dependencies": [
              "total_profit",
              "total_cash_invested"
            ],
            "label": "Total return over hold period",
            "unit": "%",
            "format": "percent",
            "display": true
          }
        },
        "outputs": [
          {
            "id": "total_return_percent",
            "label": "Total Return (hold period)",
            "variable": "total_return_percent",
            "unit": "%",
            "format": "percent"
          },
          {
            "id": "future_sale_price",
            "label": "Estimated Future Sale Price",
            "variable": "future_sale_price",
            "unit": "currency",
            "format": "currency"
          },
          {
            "id": "total_profit",
            "label": "Total Pre-Tax Profit",
            "variable": "total_profit",
            "unit": "currency",
            "format": "currency"
          }
        ]
      },
      "projected_irr": {
        "label": "Approximate Annualized Return (IRR estimate)",
        "description": "Approximate annualized return derived from cumulative profit and sale proceeds using geometric average (CAGR-style approximation). This is an approximation and not a cash-flow-solved IRR.",
        "variables": {
          "total_cash_invested": {
            "expression": "down_payment + (purchase_price * closing_costs_percent / 100)",
            "dependencies": [
              "down_payment",
              "purchase_price",
              "closing_costs_percent"
            ],
            "label": "Total initial cash invested",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "loan_amount": {
            "expression": "purchase_price - down_payment",
            "dependencies": [
              "purchase_price",
              "down_payment"
            ],
            "label": "Loan amount",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "monthly_rate": {
            "expression": "loan_interest_rate / 12 / 100",
            "dependencies": [
              "loan_interest_rate"
            ],
            "label": "Monthly interest rate",
            "format": "decimal",
            "display": false
          },
          "n_payments": {
            "expression": "loan_term_years * 12",
            "dependencies": [
              "loan_term_years"
            ],
            "label": "Number of monthly payments",
            "format": "integer",
            "display": false
          },
          "monthly_payment": {
            "expression": "loan_amount * (monthly_rate) / (1 - (1 + monthly_rate) ^ (-n_payments))",
            "dependencies": [
              "loan_amount",
              "monthly_rate",
              "n_payments"
            ],
            "label": "Estimated monthly mortgage payment",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "annual_cash_flow": {
            "expression": "((monthly_rent * (1 - vacancy_rate / 100) + other_monthly_income) * 12 - (annual_operating_expenses + property_tax_annual + insurance_annual + hoa_annual) - (monthly_payment * 12) - capital_expenditures_annual)",
            "dependencies": [
              "monthly_rent",
              "vacancy_rate",
              "other_monthly_income",
              "annual_operating_expenses",
              "property_tax_annual",
              "insurance_annual",
              "hoa_annual",
              "monthly_payment",
              "capital_expenditures_annual"
            ],
            "label": "Estimated annual pre-tax cash flow",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "cumulative_cash_flow": {
            "expression": "annual_cash_flow * hold_years",
            "dependencies": [
              "annual_cash_flow",
              "hold_years"
            ],
            "label": "Cumulative pre-tax cash flow over hold period",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "future_sale_price": {
            "expression": "purchase_price * (1 + annual_appreciation_percent / 100) ^ hold_years",
            "dependencies": [
              "purchase_price",
              "annual_appreciation_percent",
              "hold_years"
            ],
            "label": "Estimated sale price at time of sale",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "selling_costs": {
            "expression": "future_sale_price * selling_costs_percent / 100",
            "dependencies": [
              "future_sale_price",
              "selling_costs_percent"
            ],
            "label": "Estimated selling costs",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "n_paid": {
            "expression": "hold_years * 12",
            "dependencies": [
              "hold_years"
            ],
            "label": "Number of payments made by sale",
            "format": "integer",
            "display": false
          },
          "remaining_loan_balance": {
            "expression": "loan_amount * (1 + monthly_rate) ^ n_paid - monthly_payment * ((1 + monthly_rate) ^ n_paid - 1) / monthly_rate",
            "dependencies": [
              "loan_amount",
              "monthly_rate",
              "n_paid",
              "monthly_payment"
            ],
            "label": "Estimated remaining loan balance at sale",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "net_sale_proceeds": {
            "expression": "future_sale_price - selling_costs - remaining_loan_balance",
            "dependencies": [
              "future_sale_price",
              "selling_costs",
              "remaining_loan_balance"
            ],
            "label": "Net sale proceeds",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "total_profit": {
            "expression": "cumulative_cash_flow + net_sale_proceeds",
            "dependencies": [
              "cumulative_cash_flow",
              "net_sale_proceeds"
            ],
            "label": "Total pre-tax profit",
            "unit": "currency",
            "format": "currency",
            "display": false
          },
          "total_return_factor": {
            "expression": "(total_cash_invested + total_profit) / total_cash_invested",
            "dependencies": [
              "total_cash_invested",
              "total_profit"
            ],
            "label": "Total return factor (1 + total return)",
            "format": "decimal",
            "display": false
          },
          "irr_approx": {
            "expression": "total_return_factor ^ (1 / hold_years) - 1",
            "dependencies": [
              "total_return_factor",
              "hold_years"
            ],
            "label": "Approximate annualized return (CAGR-style)",
            "unit": "%",
            "format": "percent",
            "display": true
          }
        },
        "outputs": [
          {
            "id": "irr_approx",
            "label": "Approximate Annualized Return (IRR estimate)",
            "variable": "irr_approx",
            "unit": "%",
            "format": "percent"
          },
          {
            "id": "total_return_factor",
            "label": "Total Return Factor",
            "variable": "total_return_factor",
            "format": "decimal"
          }
        ]
      }
    }
  },
  "form": {
